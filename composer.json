{
    "name": "coverd/money-bundle",
    "type": "symfony-bundle",
    "description": "This is a Symfony bundle that integrates moneyphp/money library (Fowler pattern): https://github.com/moneyphp/money.",
    "keywords": ["money", "currency", "fowler", "conversion"],
    "homepage": "https://github.com/Coverd/MoneyBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Le Van",
            "homepage": "https://twitter.com/plv"
        },
        {
            "name": "Sebastien Lefebvre"
        }
    ],
    "require": {
        "php": ">=7.3",
        "symfony/framework-bundle": "^4.4|^5.2",
        "moneyphp/money": "^3.0",
        "symfony/form": "^4.4|^5.2",
        "symfony/console": "^4.4|^5.2"
    },
    "require-dev": {
        "symfony/yaml": "^4.4|^5.2",
        "symfony/browser-kit": "^4.4|^5.2",
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/doctrine-bundle": "^1.12|^2.0",
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "^4.4|^5.2",
        "symfony/twig-bundle": "^4.4|^5.2"
    },
    "autoload": {
        "psr-4": {
            "Coverd\\MoneyBundle\\": "src/"
        },
        "exclude-from-classmap": [ "/Tests/" ]
    }
}
